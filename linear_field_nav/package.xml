  <exec_depend>python3-torch</exec_depend>
  <exec_depend>python3-torchvision</exec_depend>
  <exec_depend>python3-ipopt</exec_depend>
  <exec_depend>py-trees</exec_depend>
  <exec_depend>py-trees-ros</exec_depend>
  <exec_depend>py-trees-ros-interfaces</exec_depend>
  <exec_depend>py-trees-ros-tutorials</exec_depend>
  <exec_depend>py-trees-ros-viewer</exec_depend>
  <test_depend>ament_lint_auto</test_depend>
